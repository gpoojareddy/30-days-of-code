import java.util.*;
import java.io.*;
 
 public class Solution{
 public static void main(String[] args){
 Scanner in = new Scanner(System.in);
 int n = in.nextInt();
 System.out.println(factorial(n));
 in.close();
 }
 
 private static int factorial(int n){
 if(n==1)
  return 1;
 else
  return n*factorial(n-1);
 }
 }
 
 
